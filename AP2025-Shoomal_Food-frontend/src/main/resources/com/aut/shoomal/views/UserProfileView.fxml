<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.aut.shoomal.controllers.UserProfileController">
    <padding>
        <Insets top="50" right="50" bottom="50" left="50"/>
    </padding>
    <Label fx:id="nameLabel" text="Full Name: " styleClass="my-label"/>
    <Label fx:id="phoneLabel" text="Phone: " styleClass="my-label"/>
    <Label fx:id="emailLabel" text="Email: " styleClass="my-label"/>
    <Label fx:id="roleLabel" text="Role: " styleClass="my-label"/>
    <Label fx:id="addressLabel" text="Address: " styleClass="my-label"/>
    <Label fx:id="bankInfoLabel" text="Bank Info: " styleClass="my-label"/>

    <Button text="Back to Main" onAction="#handleBackToMain" styleClass="my-button"/>
</VBox>